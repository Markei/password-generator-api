{
    "name": "markei/password-generator",
    "description": "A password genertaor API in Symfony",
    "homepage": "https://github.com/Markei/password-generator-api",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten de Keizer",
            "homepage": "http://www.maartendekeizer.nl/"
        }
    ],
    "support": {
        "issues": "https://github.com/Markei/password-generator-api/issues",
        "docs": "https://github.com/Markei/password-generator-api"
    },
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "sensio/framework-extra-bundle": "*",
        "symfony/console": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/dotenv": "^4.0",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "symfony/thanks": "^1.0",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Markei\\PasswordGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Markei\\PasswordGenerator\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C2016TG8K7BME2MMXTFS7XGY",
            "allow-contrib": false
        }
    }
}
